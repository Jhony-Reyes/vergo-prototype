{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react-native",
    "lib": ["dom", "esnext", "ES2016"],
    "moduleResolution": "node",
    "noEmit": true,
    "target": "esnext",
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": true,
    "noImplicitAny": false,
  },
  "extends": "expo/tsconfig.base",
  "exclude": ["node_modules", "babel.config.js"],

}
